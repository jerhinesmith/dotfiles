function + {
  ssh $1.colo
}

function ew {
  if [[ $2 == "mdb" || $2 == "mredis" ]]; then
    ssh ew$1$2.colo
  else
    ssh ew$1$3$2.colo
  fi
}

function doa {
  if [[ $2 == "mdb" || $2 == "mredis" ]]; then
    ssh doa$1$2.colo
  else
    ssh doa$1$3$2.colo
  fi
}

function dev {
  ssh doadev$1.colo
}
